{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/scripts"
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: test_web_pipeline",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-v",
                "scripts/test/test_web_pipeline.py"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/scripts"
            },
            "cwd": "${workspaceFolder}"
        }
        {
            "name": "Python: test_youtube_pipeline",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "args": [
                "-v",
                "scripts/test/test_youtube_pipeline.py"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/scripts"
            },
            "cwd": "${workspaceFolder}"
        }
    ]
}
